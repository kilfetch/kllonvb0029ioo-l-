<script>
    // –û—Ç–ª–æ–∂–µ–Ω–Ω–∞—è –∑–∞–≥—Ä—É–∑–∫–∞ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π
    document.addEventListener('DOMContentLoaded', function() {
        const lazyImages = document.querySelectorAll('img[loading="lazy"]');
        
        if ('loading' in HTMLImageElement.prototype) {
            // –ë—Ä–∞—É–∑–µ—Ä –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç –∞—Ç—Ä–∏–±—É—Ç loading="lazy"
            lazyImages.forEach(img => {
                img.src = img.dataset.src;
            });
        } else {
            // –†–µ–∑–µ—Ä–≤–Ω—ã–π –≤–∞—Ä–∏–∞–Ω—Ç –¥–ª—è –±—Ä–∞—É–∑–µ—Ä–æ–≤, –Ω–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞—é—â–∏—Ö lazy loading
            const lazyImageObserver = new IntersectionObserver(function(entries, observer) {
                entries.forEach(function(entry) {
                    if (entry.isIntersecting) {
                        const lazyImage = entry.target;
                        lazyImage.src = lazyImage.dataset.src;
                        lazyImageObserver.unobserve(lazyImage);
                    }
                });
            });
            
            lazyImages.forEach(function(lazyImage) {
                lazyImageObserver.observe(lazyImage);
            });
        }
    });

    // –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –∞–Ω–∏–º–∞—Ü–∏–π
    const reducedMotion = window.matchMedia('(prefers-reduced-motion: reduce)').matches;
    
    if (reducedMotion) {
        document.documentElement.style.setProperty('--animation-duration', '0.1s');
        document.documentElement.style.setProperty('--transition-duration', '0.1s');
    } else {
        document.documentElement.style.setProperty('--animation-duration', '0.5s');
        document.documentElement.style.setProperty('--transition-duration', '0.3s');
    }
    
    // –≠—Ñ—Ñ–µ–∫—Ç —Å–∫—Ä–æ–ª–ª–∏–Ω–≥–∞ –¥–ª—è –Ω–∞–≤–∏–≥–∞—Ü–∏–æ–Ω–Ω–æ–π –ø–∞–Ω–µ–ª–∏
    let lastScrollTop = 0;
    const navbar = document.getElementById('navbar');
    
    function throttle(func, limit) {
        let lastCall = 0;
        return function() {
            const now = Date.now();
            if (now - lastCall >= limit) {
                lastCall = now;
                func.apply(this, arguments);
            }
        };
    }
    
    const handleScroll = throttle(function() {
        const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
        
        if (scrollTop > 10) {
            navbar.classList.add('scrolled');
        } else {
            navbar.classList.remove('scrolled');
        }
        
        lastScrollTop = scrollTop;
    }, 100); // –í—ã–ø–æ–ª–Ω—è–µ–º –Ω–µ —á–∞—â–µ —á–µ–º —Ä–∞–∑ –≤ 100 –º—Å
    
    window.addEventListener('scroll', handleScroll);
    
    // –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è —Ä–∞–±–æ—Ç–∞ –º–æ–±–∏–ª—å–Ω–æ–≥–æ –º–µ–Ω—é
    document.addEventListener('DOMContentLoaded', function() {
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∫–Ω–æ–ø–∫–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–∏ –Ω–∞–≤–µ—Ä—Ö
        const scrollToTopBtn = document.getElementById('scrollToTop');
        if (scrollToTopBtn) {
            // –ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –∫–Ω–æ–ø–∫—É —Ç–æ–ª—å–∫–æ –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ –≤–Ω–∏–∑
            window.addEventListener('scroll', function() {
                if (window.pageYOffset > 300) {
                    scrollToTopBtn.classList.add('visible');
                } else {
                    scrollToTopBtn.classList.remove('visible');
                }
            });
            
            // –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–ª–∏–∫–∞ –¥–ª—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∏ –Ω–∞–≤–µ—Ä—Ö
            scrollToTopBtn.addEventListener('click', function() {
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
            });
        }
        
        // –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ä–∞–±–æ—Ç–∞ –º–æ–±–∏–ª—å–Ω–æ–≥–æ –º–µ–Ω—é
        const menuBar = document.getElementById('Menu-bar');
        const menuData = document.querySelector('.menu-data');
        
        if (menuBar) {
            menuBar.addEventListener('click', function(e) {
                e.preventDefault();
                
                // –ü–µ—Ä–µ–∫–ª—é—á–∞–µ–º –∫–ª–∞—Å—Å active –¥–ª—è –º–µ–Ω—é –∏ –∫–Ω–æ–ø–∫–∏
                menuData.classList.toggle('active');
                menuBar.classList.toggle('active');
                
                // –ë–ª–æ–∫–∏—Ä—É–µ–º/—Ä–∞–∑–±–ª–æ–∫–∏—Ä—É–µ–º –ø—Ä–æ–∫—Ä—É—Ç–∫—É —Å—Ç—Ä–∞–Ω–∏—Ü—ã
                if (menuData.classList.contains('active')) {
                    document.body.style.overflow = 'hidden';
                    
                    // –¢–∞–∫—Ç–∏–ª—å–Ω–∞—è –æ–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å
                    if ('vibrate' in navigator) {
                        navigator.vibrate(50);
                    }
                } else {
                    document.body.style.overflow = '';
                    
                    // –¢–∞–∫—Ç–∏–ª—å–Ω–∞—è –æ–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å –ø—Ä–∏ –∑–∞–∫—Ä—ã—Ç–∏–∏
                    if ('vibrate' in navigator) {
                        navigator.vibrate(30);
                    }
                }
            });
        }
        
        // –ó–∞–∫—Ä—ã—Ç–∏–µ –º–µ–Ω—é –ø—Ä–∏ –∫–ª–∏–∫–µ –Ω–∞ —Å—Å—ã–ª–∫—É —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
        const menuLinks = document.querySelectorAll('.menu-data a');
        menuLinks.forEach(link => {
            link.addEventListener('click', function(e) {
                // –î–æ–±–∞–≤–ª—è–µ–º —ç—Ñ—Ñ–µ–∫—Ç –Ω–∞–∂–∞—Ç–∏—è –ø–µ—Ä–µ–¥ –∑–∞–∫—Ä—ã—Ç–∏–µ–º –º–µ–Ω—é
                this.classList.add('pressed');
                
                // –ü–æ—Å–ª–µ –Ω–µ–±–æ–ª—å—à–æ–π –∑–∞–¥–µ—Ä–∂–∫–∏ –¥–ª—è —ç—Ñ—Ñ–µ–∫—Ç–∞ –∑–∞–∫—Ä—ã–≤–∞–µ–º –º–µ–Ω—é
                setTimeout(() => {
                menuData.classList.remove('active');
                    menuBar.classList.remove('active');
                    document.body.style.overflow = '';
                    this.classList.remove('pressed');
                }, 150);
                
                // –í–∏–±—Ä–∞—Ü–∏—è –ø—Ä–∏ –≤—ã–±–æ—Ä–µ –ø—É–Ω–∫—Ç–∞ –º–µ–Ω—é
                if ('vibrate' in navigator) {
                    navigator.vibrate([15, 10, 15]);
                }
        });
    });
    
        // –ó–∞–∫—Ä—ã—Ç–∏–µ –º–µ–Ω—é –ø—Ä–∏ –∫–ª–∏–∫–µ –≤–Ω–µ –º–µ–Ω—é
        document.addEventListener('click', function(e) {
            if (menuData && menuData.classList.contains('active') && 
                !menuData.contains(e.target) && 
                e.target !== menuBar && 
                !menuBar.contains(e.target)) {
            menuData.classList.remove('active');
                menuBar.classList.remove('active');
                document.body.style.overflow = '';
            }
        });
        
        // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –∫–ª–∞—Å—Å–æ–≤ –∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–∏ –¥–ª—è —Ç–µ–∫—É—â–µ–π —Å—Ç—Ä–∞–Ω–∏—Ü—ã –≤ –º–µ–Ω—é
        const currentPage = window.location.pathname;
        menuLinks.forEach(link => {
            const linkPath = link.getAttribute('href');
            if (linkPath && currentPage.includes(linkPath) && linkPath !== '/') {
                link.classList.add('active');
            } else if (currentPage === '/' && linkPath === '/index.html') {
                link.classList.add('active');
            }
        });
        
        // –î–æ–±–∞–≤–ª—è–µ–º –∏–Ω–¥–µ–∫—Å—ã –¥–ª—è –∞–Ω–∏–º–∞—Ü–∏–∏ —ç–ª–µ–º–µ–Ω—Ç–æ–≤
        const apiEndpoints = document.querySelectorAll('.api-endpoint');
        apiEndpoints.forEach((endpoint, index) => {
            endpoint.style.setProperty('--i', index);
        });
        
        // –£–ª—É—á—à–∞–µ–º –ø—Ä–æ–∫—Ä—É—Ç–∫—É –¥–ª—è –∫–æ–¥–∞ —Å –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ–º –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä–∞
        const codeExamples = document.querySelectorAll('.code-example pre');
        codeExamples.forEach(code => {
            // –ü—Ä–æ–≤–µ—Ä—è–µ–º, —Ç—Ä–µ–±—É–µ—Ç—Å—è –ª–∏ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω–∞—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∞
            if (code.scrollWidth > code.clientWidth) {
                code.parentElement.classList.add('scrollable-block');
            }
            
            // –î–æ–±–∞–≤–ª—è–µ–º –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω–æ–π –ø—Ä–æ–∫—Ä—É—Ç–∫–∏
            code.addEventListener('scroll', function() {
                if (this.scrollWidth > this.clientWidth) {
                    if (this.scrollLeft > 0) {
                        this.classList.add('scrolled');
                    } else {
                        this.classList.remove('scrolled');
                    }
                }
            });
        });
        
        // –£–ª—É—á—à–µ–Ω–Ω–∞—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∞ –∫ —è–∫–æ—Ä—è–º —Å –ø–ª–∞–≤–Ω–æ–π –∞–Ω–∏–º–∞—Ü–∏–µ–π
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                    
                    // –ü–æ–¥—Å–≤–µ—Ç–∫–∞ —Ü–µ–ª–µ–≤–æ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–∞ –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ –∫ –Ω–µ–º—É
                    target.classList.add('highlight-target');
                    setTimeout(() => {
                        target.classList.remove('highlight-target');
                    }, 1500);
                }
            });
        });
        
        // –†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è —Å–µ—Ä–≤–∏—Å-–≤–æ—Ä–∫–µ—Ä–∞ –¥–ª—è –ø–æ–¥–¥–µ—Ä–∂–∫–∏ –æ—Ñ—Ñ–ª–∞–π–Ω-—Ä–µ–∂–∏–º–∞
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', function() {
                navigator.serviceWorker.register('/service-worker.js').then(function(registration) {
                    console.log('ServiceWorker registered: ', registration.scope);
                }).catch(function(error) {
                    console.log('ServiceWorker registration failed: ', error);
                });
            });
        }
    });

    // –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –æ—Ç—Ä–∏—Å–æ–≤–∫–∏ DOM
    document.addEventListener('DOMContentLoaded', function() {
        // –û—Ç–ª–æ–∂–µ–Ω–Ω–∞—è –∑–∞–≥—Ä—É–∑–∫–∞ –Ω–µ–≤–∞–∂–Ω—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤
        const deferredElements = document.querySelectorAll('[data-defer]');
        
        if ('IntersectionObserver' in window) {
            const observer = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        const element = entry.target;
                        if (element.dataset.src) {
                            element.src = element.dataset.src;
                            delete element.dataset.src;
                        }
                        if (element.dataset.srcset) {
                            element.srcset = element.dataset.srcset;
                            delete element.dataset.srcset;
                        }
                        if (element.dataset.background) {
                            element.style.backgroundImage = `url(${element.dataset.background})`;
                            delete element.dataset.background;
                        }
                        if (element.dataset.class) {
                            element.classList.add(element.dataset.class);
                            delete element.dataset.class;
                        }
                        observer.unobserve(element);
                    }
                });
            }, {
                rootMargin: '100px 0px' // –ó–∞–≥—Ä—É–∂–∞—Ç—å —ç–ª–µ–º–µ–Ω—Ç—ã –ø—Ä–∏ –ø—Ä–∏–±–ª–∏–∂–µ–Ω–∏–∏ –∫ –Ω–∏–º –Ω–∞ 100px
            });
            
            deferredElements.forEach(element => {
                observer.observe(element);
            });
        } else {
            // –†–µ–∑–µ—Ä–≤–Ω—ã–π –≤–∞—Ä–∏–∞–Ω—Ç –¥–ª—è —Å—Ç–∞—Ä—ã—Ö –±—Ä–∞—É–∑–µ—Ä–æ–≤
            deferredElements.forEach(element => {
                if (element.dataset.src) element.src = element.dataset.src;
                if (element.dataset.srcset) element.srcset = element.dataset.srcset;
                if (element.dataset.background) element.style.backgroundImage = `url(${element.dataset.background})`;
                if (element.dataset.class) element.classList.add(element.dataset.class);
            });
        }
        
        // –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è —Ä–∞–±–æ—Ç—ã —Å DOM
        // –ì—Ä—É–ø–ø–∏—Ä–æ–≤–∫–∞ –Ω–µ—Å–∫–æ–ª—å–∫–∏—Ö –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π DOM –≤ –æ–¥–Ω–æ –¥–ª—è –ø—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏—è –∏–∑–ª–∏—à–Ω–∏—Ö –ø–µ—Ä–µ—Ä–∏—Å–æ–≤–æ–∫
        function batchDOMUpdates(callback) {
            requestAnimationFrame(() => {
                const startTime = performance.now();
                callback();
                const endTime = performance.now();
                console.log(`Batch DOM update took ${endTime - startTime}ms`);
            });
        }
        
        // –ü—Ä–∏–º–µ—Ä –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è batchDOMUpdates –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤
        function createAPIEndpoint(data) {
            const endpoint = document.createElement('div');
            endpoint.className = 'api-endpoint';
            // ... –∑–∞–ø–æ–ª–Ω–µ–Ω–∏–µ —Å–æ–¥–µ—Ä–∂–∏–º—ã–º ...
            return endpoint;
        }
        
        // –ü—Ä–∏–º–µ—Ä –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è —Ñ—Ä–∞–≥–º–µ–Ω—Ç–∞ –¥–ª—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏ –≤—Å—Ç–∞–≤–∫–∏ –≤ DOM
        function addAPIEndpoints(endpoints, container) {
            batchDOMUpdates(() => {
                const fragment = document.createDocumentFragment();
                endpoints.forEach((endpointData, index) => {
                    const endpoint = createAPIEndpoint(endpointData);
                    endpoint.style.setProperty('--i', index);
                    fragment.appendChild(endpoint);
                });
                container.appendChild(fragment);
            });
        }
        
        // –£–º–µ–Ω—å—à–µ–Ω–∏–µ —Ä–∞–±–æ—Ç—ã –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –ø–æ—Ç–æ–∫–∞
        // –ó–∞–º–µ–Ω—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–∏ –¥–ª—è —É–ª—É—á—à–µ–Ω–∏—è –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        let ticking = false;
        window.addEventListener('scroll', function() {
            if (!ticking) {
                requestAnimationFrame(function() {
                    const scrollPos = window.scrollY;
                    // –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–æ–±—ã—Ç–∏—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∏
                    const navbar = document.getElementById('navbar');
                    if (scrollPos > 10) {
                        navbar.classList.add('scrolled');
                    } else {
                        navbar.classList.remove('scrolled');
                    }
                    ticking = false;
                });
                ticking = true;
            }
        }, { passive: true }); // –ò—Å–ø–æ–ª—å–∑—É–µ–º –ø–∞—Å—Å–∏–≤–Ω—ã–µ —Å–ª—É—à–∞—Ç–µ–ª–∏ —Å–æ–±—ã—Ç–∏–π –¥–ª—è —É–ª—É—á—à–µ–Ω–∏—è –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        
        // –≠–∫–æ–Ω–æ–º–∏—è —Å–µ—Ç–µ–≤–æ–≥–æ —Ç—Ä–∞—Ñ–∏–∫–∞
        // –ó–∞–≥—Ä—É–∑–∫–∞ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π –≤ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–µ–º —Ä–∞–∑—Ä–µ—à–µ–Ω–∏–∏
        function loadAppropriateImage() {
            const screenWidth = window.innerWidth;
            const images = document.querySelectorAll('[data-srcset]');
            
            images.forEach(img => {
                const srcset = img.dataset.srcset.split(',');
                let bestSrc = '';
                let bestWidth = 0;
                
                srcset.forEach(src => {
                    const [url, width] = src.trim().split(' ');
                    const numWidth = parseInt(width);
                    
                    if (numWidth >= screenWidth && (bestWidth === 0 || numWidth < bestWidth)) {
                        bestWidth = numWidth;
                        bestSrc = url;
                    }
                });
                
                if (bestSrc) {
                    img.src = bestSrc;
                } else if (srcset.length > 0) {
                    const fallback = srcset[srcset.length - 1].trim().split(' ')[0];
                    img.src = fallback;
                }
            });
        }
        
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–π
        loadAppropriateImage();
        window.addEventListener('resize', throttle(loadAppropriateImage, 200), { passive: true });
        
        // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏—è —á–∞—Å—Ç–æ—Ç—ã –≤—ã–∑–æ–≤–∞
        function throttle(func, limit) {
            let lastCall = 0;
            return function() {
                const now = Date.now();
                if (now - lastCall >= limit) {
                    lastCall = now;
                    func.apply(this, arguments);
                }
            };
        }
    });

    // –°–æ–∫—Ä–∞—â–µ–Ω–∏–µ –≤—Ä–µ–º–µ–Ω–∏ –æ–±—Ä–∞–±–æ—Ç–∫–∏ JavaScript
    // –ü–æ–¥–≥—Ä—É–∑–∫–∞ –Ω–µ–æ–±—è–∑–∞—Ç–µ–ª—å–Ω—ã—Ö —Å–∫—Ä–∏–ø—Ç–æ–≤ —Ç–æ–ª—å–∫–æ –ø—Ä–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏
    function loadScriptOnDemand(url, callback) {
        const script = document.createElement('script');
        script.src = url;
        script.onload = callback || function() {};
        document.head.appendChild(script);
    }

    // –ü—Ä–∏–º–µ—Ä: –∑–∞–≥—Ä—É–∑–∫–∞ —Å–∫—Ä–∏–ø—Ç–∞ –¥–ª—è —Å–∏–Ω—Ç–∞–∫—Å–∏—á–µ—Å–∫–æ–π –ø–æ–¥—Å–≤–µ—Ç–∫–∏ —Ç–æ–ª—å–∫–æ –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ –¥–æ –±–ª–æ–∫–∞ —Å –∫–æ–¥–æ–º
    function loadSyntaxHighlighter() {
        const codeBlocks = document.querySelectorAll('.code-example');
        if (codeBlocks.length === 0) return;
        
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    loadScriptOnDemand("/js/syntax-highlighter.js", function() {
                        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø–æ–¥—Å–≤–µ—Ç–∫–∏ —Å–∏–Ω—Ç–∞–∫—Å–∏—Å–∞
                        if (window.highlightCode) {
                            window.highlightCode();
                        }
                    });
                    observer.disconnect(); // –ó–∞–≥—Ä—É–∂–∞–µ–º —Ç–æ–ª—å–∫–æ –æ–¥–∏–Ω —Ä–∞–∑
                }
            });
        });
        
        // –ù–∞–±–ª—é–¥–∞–µ–º –∑–∞ –ø–µ—Ä–≤—ã–º –±–ª–æ–∫–æ–º –∫–æ–¥–∞
        observer.observe(codeBlocks[0]);
    }

    // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –æ—Ç–ª–æ–∂–µ–Ω–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏ —Å–∫—Ä–∏–ø—Ç–æ–≤
    document.addEventListener('DOMContentLoaded', function() {
        loadSyntaxHighlighter();
    });
</script>
