            color: #ffffff;
            border-left: 3px solid rgba(200, 200, 200, 0.5);
        }
        
        .method-post {
            background-color: rgba(160, 160, 160, 0.15);
            color: #e0e0e0;
        }
        
        .method-put {
            background-color: rgba(160, 160, 160, 0.15);
            color: #e0e0e0;
        }
        
        .method-delete {
