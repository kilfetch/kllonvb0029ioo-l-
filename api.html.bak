        }
        
        .endpoint-method {
            display: inline-block;
            padding: 6px 12px;
            border-radius: 6px;
            font-weight: 600;
            font-size: 0.9rem;
            margin-right: 12px;
            font-family: 'JetBrains Mono', monospace;
            text-align: left;
            letter-spacing: 0.5px;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
        }
        
        .method-get {
