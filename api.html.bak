<script>
// –ú–æ–±–∏–ª—å–Ω–æ–µ –º–µ–Ω—é
document.addEventListener('DOMContentLoaded', function() {
    const menuBar = document.getElementById('Menu-bar');
    const menuData = document.querySelector('.menu-data');
    
    if (menuBar && menuData) {
        menuBar.addEventListener('click', function() {
            menuData.classList.toggle('active');
        });
        
        // –ó–∞–∫—Ä—ã—Ç–∏–µ –º–µ–Ω—é –ø—Ä–∏ –∫–ª–∏–∫–µ –Ω–∞ —Å—Å—ã–ª–∫—É
        const menuLinks = menuData.querySelectorAll('a');
        menuLinks.forEach(link => {
            link.addEventListener('click', function() {
                menuData.classList.remove('active');
            });
        });
        
        // –ó–∞–∫—Ä—ã—Ç–∏–µ –º–µ–Ω—é –ø—Ä–∏ –∫–ª–∏–∫–µ –≤–Ω–µ –µ–≥–æ
        document.addEventListener('click', function(event) {
            if (!menuData.contains(event.target) && event.target !== menuBar) {
                menuData.classList.remove('active');
            }
        });
    }
    
    // –ü–æ–¥—Å–≤–µ—Ç–∫–∞ –∞–∫—Ç–∏–≤–Ω–æ–≥–æ –ø—É–Ω–∫—Ç–∞ –º–µ–Ω—é
    const currentPath = window.location.pathname;
    const navLinks = document.querySelectorAll('.nav-button, .footer-links a');
    
    navLinks.forEach(link => {
        const linkPath = link.getAttribute('href');
        if (currentPath === linkPath || 
            (currentPath.includes(linkPath) && linkPath !== '/')) {
            link.classList.add('active');
        }
    });
});
</script>

<script>
// –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–ª—è –≤–∫–ª–∞–¥–æ–∫ –≤ –ø—Ä–∏–º–µ—Ä–µ –æ—Ç–≤–µ—Ç–∞ API
document.addEventListener('DOMContentLoaded', function() {
    // –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –≤–∫–ª–∞–¥–æ–∫ –æ—Ç–≤–µ—Ç–∞ API
    const tabs = document.querySelectorAll('.response-tab');
    if (tabs.length > 0) {
        tabs.forEach(tab => {
            tab.addEventListener('click', function() {
                const tabsContainer = this.closest('.response-tabs').parentElement;
                
                // –£–¥–∞–ª—è–µ–º –∞–∫—Ç–∏–≤–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ —É –≤—Å–µ—Ö –≤–∫–ª–∞–¥–æ–∫ –≤ —Ç–µ–∫—É—â–µ–º –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–µ
                tabsContainer.querySelectorAll('.response-tab').forEach(t => {
                    t.classList.remove('active');
                });
                
                // –°–∫—Ä—ã–≤–∞–µ–º –≤—Å–µ –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä—ã —Å–æ–¥–µ—Ä–∂–∏–º–æ–≥–æ –≤ —Ç–µ–∫—É—â–µ–º –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–µ
                tabsContainer.querySelectorAll('.response-tab-content').forEach(c => {
                    c.classList.remove('active');
                });
                
                // –î–æ–±–∞–≤–ª—è–µ–º –∞–∫—Ç–∏–≤–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ —Ç–µ–∫—É—â–µ–π –≤–∫–ª–∞–¥–∫–µ
                this.classList.add('active');
                
                // –û–ø—Ä–µ–¥–µ–ª—è–µ–º, –∫–∞–∫—É—é –≤–∫–ª–∞–¥–∫—É –ø–æ–∫–∞–∑–∞—Ç—å
                const tabName = this.getAttribute('data-tab');
                tabsContainer.querySelector(`.response-tab-content#${tabName}-tab`).classList.add('active');
            });
        });
    }
    
    // –î–æ–±–∞–≤–ª—è–µ–º –∫–Ω–æ–ø–∫—É "–ù–∞–≤–µ—Ä—Ö", –µ—Å–ª–∏ –µ—ë –Ω–µ—Ç
    if (!document.getElementById('scrollToTop')) {
        const scrollButton = document.createElement('div');
        scrollButton.id = 'scrollToTop';
        scrollButton.className = 'scroll-to-top';
        scrollButton.innerHTML = '<i class="fas fa-arrow-up"></i>';
        document.body.appendChild(scrollButton);
        
        // –ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –∫–Ω–æ–ø–∫—É —Ç–æ–ª—å–∫–æ –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ –≤–Ω–∏–∑
        window.addEventListener('scroll', function() {
            if (window.pageYOffset > 300) {
                scrollButton.classList.add('visible');
            } else {
                scrollButton.classList.remove('visible');
            }
        });
        
        // –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–ª–∏–∫–∞ –¥–ª—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∏ –Ω–∞–≤–µ—Ä—Ö
        scrollButton.addEventListener('click', function() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            });
        });
    }
    
    // –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ —Å–∫—Ä–∏–ø—Ç–∞ —Å–∏–Ω—Ç–∞–∫—Å–∏—á–µ—Å–∫–æ–π –ø–æ–¥—Å–≤–µ—Ç–∫–∏
    if (typeof loadSyntaxHighlighter === 'function') {
        loadSyntaxHighlighter();
    }
});
</script>

